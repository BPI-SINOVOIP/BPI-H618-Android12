    <!-- Control whether the normal led flick when ir input.
            0 - both not flick.
            1 - standby led flick , normal led not flick.
            2 - normal led flick , standby led flick.
            3 - both flick.
    -->
    <integer name="config_ledNormalFlicker">1</integer>

    <!-- Control whether all led off when standby.
            false - not all led off, the standby led on.
            true - all led off.
    -->
    <bool name="config_ledStandbyAllOff">false</bool>

    <!-- Control whether all led off when shutdown.
            false - not all led off, the standby led on.
            true -  all led off.
    -->
    <bool name="config_ledShutdownAllOff">false</bool>

    <bool name="config_shutdownForceScreenOff">false</bool>

    <!-- Control gpio write low level or high level when the led on
            false - led on when input high level
            true - led on when input low level
    -->
    <bool name="config_ledBrightInLowLevel">false</bool>
</resources>
